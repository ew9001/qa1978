 <!DOCTYPE suite SYSTEM "http://beust.com/testng/testng-1.0.dtd" >
 
<suite name="Add to Cart Random Products" verbose="1">
    <parameter name="param" value="Test"/>
   
    
     
    
     <test name="Add to Cart  iPhone 4S">
        <parameter name="browser" value="mobileiPhone5S"/>
        <classes>
            <class name="brand.charmin.CharminAddCartMobile"/>
        </classes>
 
        <groups>
            <run>
                <include name="create"/>
            </run>
        </groups>
    </test>
    <test name="Add to Cart Galaxy S3">
        <parameter name="browser" value="mobileGalaxyS3"/>
        <classes>
            <class name="brand.charmin.CharminAddCartMobile"/>
        </classes>
 
        <groups>
            <run>
                <include name="create"/>
            </run>
        </groups>
    </test>
    
     <test name="Add to Cart S4">
        <parameter name="browser" value="mobileGalaxyS4"/>
        <classes>
            <class name="brand.charmin.CharminAddCartMobile"/>
        </classes>
 
        <groups>
            <run>
                <include name="create"/>
            </run>
        </groups>
    </test>
    
</suite>

