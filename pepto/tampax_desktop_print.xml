  <!DOCTYPE suite SYSTEM "http://beust.com/testng/testng-1.0.dtd" >
 
<suite name="Print IE10" verbose="1">
    <parameter name="param" value="Test"/>
    <test name="ObjectExitTest simple example">
        <parameter name="browser" value="browserIE10"/>
        <classes>
            <class name="brand.tampax.production.ArticlesPrint"/>
        </classes>
 
        <groups>
            <run>
                <include name="create"/>
            </run>
        </groups>
    </test>
    <test name="Print Firefox">
        <parameter name="browser" value="firefox"/>
        <classes>
            <class name="brand.tampax.production.ArticlesPrint"/>
        </classes>
 
        <groups>
            <run>
                <include name="create"/>
            </run>
        </groups>
    </test>
    
     <test name="Print IE11">
        <parameter name="browser" value="browserIE11"/>
        <classes>
            <class name="brand.tampax.production.ArticlesPrint"/>
        </classes>
 
        <groups>
            <run>
                <include name="create"/>
            </run>
        </groups>
    </test>
    
    
</suite>
 