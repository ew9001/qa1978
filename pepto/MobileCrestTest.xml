  <!DOCTYPE suite SYSTEM "http://beust.com/testng/testng-1.0.dtd" >
 
<suite name="Add to Cart" verbose="1">
    <parameter name="param" value="Test"/>
    <test name="Add to Cart Galaxy S4 ">
        <parameter name="browser" value="mobileGalaxyS4"/>
        <classes>
            <class name="brand.crest.MobileCrestURL"/>
        </classes>
 
        <groups>
            <run>
                <include name="create"/>
            </run>
        </groups>
    </test>
    
    <test name="iPhone 5">
        <parameter name="browser" value="mobileiPhone5S"/>
        <classes>
            <class name="brand.crest.MobileCrestURL"/>
        </classes>
 
        <groups>
            <run>
                <include name="create"/>
            </run>
        </groups>
    </test>

    
     
    
</suite>
