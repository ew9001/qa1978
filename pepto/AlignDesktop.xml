 <!DOCTYPE suite SYSTEM "http://beust.com/testng/testng-1.0.dtd" >
 
<suite name="Browser Print " verbose="1">
    <parameter name="param" value="Test"/>
    
     <test name="Screen grab on browser  tabletIpad4thGen7">
        <parameter name="browser" value=" tabletIpad4thGen7"/>
        <classes>
            <class name="brand.align.AlignDesktopScreen"/>
        </classes>
 
        <groups>
            <run>
                <include name="create"/>
            </run>
        </groups>
    </test>
    
    <test name="Screen grab on browser Firefox">
        <parameter name="browser" value="firefox"/>
        <classes>
            <class name="brand.align.AlignDesktopScreen"/>
        </classes>
 
        <groups>
            <run>
                <include name="create"/>
            </run>
        </groups>
    </test>
    
    <test name="Screen grab on browser  IE10">
        <parameter name="browser" value="browserIE10"/>
        <classes>
            <class name="brand.align.AlignDesktopScreen"/>
        </classes>
 
        <groups>
            <run>
                <include name="create"/>
            </run>
        </groups>
    </test>
    
    <test name="Screen grab on browser IE11">
        <parameter name="browser" value="browserIE11"/>
        <classes>
            <class name="brand.align.AlignDesktopScreen"/>
        </classes>
 
        <groups>
            <run>
                <include name="create"/>
            </run>
        </groups>
    </test>
    
     
    
    
    
   
    
    
</suite>
